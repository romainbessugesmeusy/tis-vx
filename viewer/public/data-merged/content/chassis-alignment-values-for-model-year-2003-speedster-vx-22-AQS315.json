  "htmlContent": "<div class=\"content\">\n\r\n<table width=\"100%\" >\r\n  <!--XPRINT-->\r\n  <table>\r\n  <tr>\r\n    <td></td>\r\n   <td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</td>\r\n  \t\r\n  </tr>\r\n  </table>\r\n  \r\n  <!--/XPRINT-->\r\n  <tr>\r\n    <td >\n<p class=\"title\">Chassis Alignment Values for Model Year 2003\nSpeedster/VX 220</p>\n\n<p class=\"para-before-table\">The values apply to vehicles with 70\nkg/154 lbs. on each front seat and a half-full fuel tank.</p>\n\n<table cellspacing=\"0\" cellpadding=\"2\" class=\"border-all\">\n<colgroup>\n<col width=\"31%\">\n<col width=\"51%\">\n<col width=\"18%\"></colgroup>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"top\">\n<p class=\"bold\">Front wheel adjustment</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p class=\"bold\">Roadster</p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p class=\"bold\">Difference right/left maximum</p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p>Camber</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>-0&deg; 06' <sup class=\"sup\">+6'</sup> / <sub class=\n\"sub\">-12'</sub></p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p><span class=\"tech-spec\" style=\n\"background:lightgrey; border:2; border-style:dotted; font-weight:bold\">\n12'</span></p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p>Caster</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>3&deg; 48' &plusmn;12'</p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p><span class=\"tech-spec\" style=\n\"background:lightgrey; border:2; border-style:dotted; font-weight:bold\">\n12'</span></p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p>Toe-in <a class=\"ftnref\" href=\"#1\">1)</a></p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>-0&deg; 20' <sup class=\"sup\">+20'</sup></p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p>-</p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p>Inclination</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>12&deg; 00'</p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p>-</p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p>Toe-in variation with inner wheel deflection of 20&deg;</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>-</p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p>-</p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p class=\"bold\">Rear wheel adjustment</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>&nbsp;</p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p class=\"bold\">&nbsp;</p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-botright\" align=\"left\" valign=\"middle\">\n<p>Camber</p>\n</td>\n<td class=\"border-botright\" align=\"center\" valign=\"top\">\n<p>-1&deg; 48' &plusmn;12'</p>\n</td>\n<td class=\"border-bot\" align=\"center\" valign=\"top\">\n<p><span class=\"tech-spec\" style=\n\"background:lightgrey; border:2; border-style:dotted; font-weight:bold\">\n12'</span></p>\n</td>\n</tr>\n\n<tr valign=\"top\">\n<td class=\"border-right\" align=\"left\" valign=\"middle\">\n<p>Toe-in <a class=\"ftnref\" href=\"#1\">1)</a> &nbsp;</p>\n</td>\n<td class=\"border-right\" align=\"center\" valign=\"top\">\n<p><span class=\"tech-spec\" style=\n\"background:lightgrey; border:2; border-style:dotted; font-weight:bold\">\n+0&deg; 12'</span> <sup class=\"sup